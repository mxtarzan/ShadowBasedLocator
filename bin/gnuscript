#first trace
set xrange [-180:180]
set yrange [-90:90]
set ylabel "Latitude"
set xlabel "Longitude"
set size ratio -1

set style line 2 \
    linecolor rgb '#dd181f' \
    linetype 1 linewidth 2 \
    pointtype 5 pointsize 1
set encoding utf8

set minussign
set grid ytics lt 0 lw 1 lc rgb "#bbbbbb"
set grid xtics lt 0 lw 1 lc rgb "#bbbbbb"
plot 'trace9.dat' with points linestyle 2
