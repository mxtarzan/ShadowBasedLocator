set title "Shadow Based Locator"
set xrange [-180:180]
set yrange [-90:90]
set ylabel "Latitude"
set xlabel "Longitude"
set size ratio -1

set encoding utf8

set minussign
set grid ytics lt 0 lw 1 lc rgb "#bbbbbb"
set grid xtics lt 0 lw 1 lc rgb "#bbbbbb"
plot './src/Data25.dat'  	    with points linecolor rgb '#ff0000'  pointtype 2 pointsize 0.25,'./src/Data26.dat' 		with points linecolor rgb '#00ff00'  pointtype 3 pointsize 0.25,'./src/Data27.dat' 		with points linecolor rgb '#0000ff'  pointtype 4 pointsize 0.25,'./src/ActualLocation.dat' with points linecolor rgb '#000000' pointtype 1 pointsize 0.25
