set xrange [-180:180]
set yrange [-90:90]
set ylabel "Latitude"
set xlabel "Longitude"
set size ratio -1

set style line 2 \
    linecolor rgb '#000000' \
    linetype 1 linewidth 2 \
    pointtype 1 pointsize 0.75
set style line 3 \
    linecolor rgb '#ff00ff' \
    linetype 1 linewidth 2 \
    pointtype 1 pointsize 0.75
set style line 2 \
    linecolor rgb '#00ffff' \
    linetype 1 linewidth 2 \
    pointtype 1 pointsize 0.75
set encoding utf8

set minussign
set grid ytics lt 0 lw 1 lc rgb "#bbbbbb"
set grid xtics lt 0 lw 1 lc rgb "#bbbbbb"
plot 'trace1.dat' with points linestyle 2 linecolor rgb '#000000', 'trace9.dat' with points linestyle 3 linecolor rgb '#ff00ff', 'trace10.dat' with points linestyle 4 linecolor rgb '#00ffff'
